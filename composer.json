{
  "name": "bradjonesllc/simplesamlphp-module-openidconnect",
  "description": "Provides OpenID Connect support to SimpleSAMLphp",
  "license": "GPL-2.0+",
  "keywords": ["authentication"],
  "type": "simplesamlphp-module",
  "homepage": "https://github.com/BradJonesLLC/simplesamlphp-module-openidconnect",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/bradjones1/php-openid-connect-client"
    }
  ],
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Brad Jones"
    }
  ],
  "require": {
    "ivan-novakov/php-openid-connect-client": "dev-httpClient-fix",
    "simplesamlphp/composer-module-installer": "~1.1.4",
    "simplesamlphp/simplesamlphp": "1.*"
  },
  "autoload": {
    "classmap": ["src/Auth/Source/"],
    "psr-4": {"SSPOpenIdConnect\\": "src"}
  }
}
